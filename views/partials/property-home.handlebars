<a href="/property/{{id}}">
  <div class="col">
    <div class="dash-card home-card">
      <div class="dash-card-badge">
        <p class="dash-card-rent">${{monthly_rent}} / mo</p>
      </div>
      <div class="dash-card-home-image">
        <img
          src="https://tenantively-images.s3.amazonaws.com/{{property_image}}"
          class="card-img-top home-card-img-top"
          alt="An image of the house at {{address}}"
        />
      </div>
      <div class="home-card-title">
        <h2>{{address}}</h2>
      </div>
    </div>
  </div>
</a>